<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.tamk0002" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>tamk0002</name>
    <description>
        MAD9022 Midterm - GIFTR
        March 31st 2017
        Christine Tamkican
    </description>
    <author email="tamk0002@algonquinlive.com" href="http://cordova.io">
        Christine Tamican
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="port-hdpi" src="res/screen/giftr-splashscreen.png" />
    <icon density="hdpi" src="res/icons/giftr-icon.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplshScreenDelay" value="5000" />
</widget>
